%table.table
  %thead
    %tr
      %th Type
      %th Active
      %th Actions
  - @warehouse_types.each do |warehouse_type|
    %tr
      %td= warehouse_type.name
      %td
        - if warehouse_type.active?
          = "active"
        - else
          = "inactive"
      %td
        / - if can? :update, warehouse_type
        = link_to "Edit", edit_warehouse_type_path(warehouse_type)
