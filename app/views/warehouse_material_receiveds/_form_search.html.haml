= simple_form_for @warehouse_material_received, url: warehouse_material_receiveds_path, method: :get, html: {novalidate: true, class: 'form-inline'} do |f|
  .input-group.col-xs-5
    = f.input :warehouse_item_transaction_id, as: :string, label: false, input_html: {class: "form-control", placeholder: "Search by requested number"}
    .input-group-btn
      = f.submit :Search, class: "btn btn-default"
