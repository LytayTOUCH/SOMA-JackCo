%table#list_data_table
  %thead
    %tr
      %th.col-md-2= "Time"
      %th.col-md-1= "User"
      %th.col-md-2= "Description"
      %th.col-md-7= "Object"
  %tbody
    - @logs.each do |log|
      %tr
        %td= log.created_at
        %td= log.user.email
        %td= log.note
        %td
          %pre= log.object
